  "description": "DLT Managers",
  "main": "./dist/index.js",
  "author": "ArbÃ«r Salihi",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "shx rm -rf bundles dist",
    "dev": "npm run build-dev && npm run start-dev",
    "dev-cli": "npm run build-dev && npm run start-cli",
    "pre:dev": "copyfiles -u 1 'src/scripts/*' dist",
    "start-dev": "sudo nodemon -r tsconfig-paths/register --exec ts-node -- ./src/index.ts",
    "build-dev": "npm run clean && npm run webpack-dev && ttsc --declaration --declarationMap",
    "webpack-dev": "webpack --mode=development",
    "prod": "npm run build-prod && npm run start-prod",
    "prod-cli": "npm run build-prod && npm run start-cli",
    "pre:prod": "copyfiles -u 1 'src/scripts/*' dist",
    "start-prod": "sudo node -r ts-node/register -r tsconfig-paths/register ./dist/index.js",
    "start-cli": "sudo node -r ts-node/register -r tsconfig-paths/register ./dist/cli.js",
    "build-prod": "npm run clean && npm run pre:prod && ttsc --declaration --declarationMap",
    "webpack-prod": "webpack --mode=production"
  },
  "devDependencies": {
    "@types/amqplib": "^0.8.2",
